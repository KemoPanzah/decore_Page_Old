  "version": "0.2.0",
  "configurations": [
    {
      "name": "Sphinx Make Translate (en)",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": ".venv/Scripts/sphinx-build.exe",
      "args": [
        "-b",
        "gettext",
        "source",
        "source/_text",
        "-a",
        "-E",
        "-D",
        "language=en"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Sphinx Make Dev (de)",
      "type": "debugpy",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": ".venv/Scripts/sphinx-build.exe",
