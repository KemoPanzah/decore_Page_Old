  "version": "0.2.0",
  "configurations": [
    {
      "name": "Sphinx Make Translate",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": ".venv/Scripts/sphinx-build.exe",
      "args": ["-b", "gettext", "source", "source/_text", "-a", "-E"],
      "console": "integratedTerminal"
    },
    {
      "name": "Sphinx Make Dev",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": ".venv/Scripts/sphinx-build.exe",
      "args": ["-b", "html", "source", "dev_docs", "-a", "-E"],
      "console": "integratedTerminal"
    },
    {
      "name": "Sphinx Make Page",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": ".venv/Scripts/sphinx-build.exe",
      "args": ["-b", "html", "source", "docs", "-a", "-E"],
      "console": "integratedTerminal"
    },
    {
      "name": "Sphinx Make Readme",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": ".venv/Scripts/sphinx-build.exe",
      "args": ["-b", "rst", "source", "readme", "-a", "-E"],
      "console": "integratedTerminal"
    }
  ]
}
