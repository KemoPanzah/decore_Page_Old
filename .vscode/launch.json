{
  // Verwendet IntelliSense zum Ermitteln möglicher Attribute.
  // Zeigen Sie auf vorhandene Attribute, um die zugehörigen Beschreibungen anzuzeigen.
  // Weitere Informationen finden Sie unter https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Sphinx Make Translate (en)",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": ".venv/Scripts/sphinx-build.exe",
      "args": ["-b", "gettext", "source", "source/_text", "-a", "-E", "-D", "language=en"],
      "console": "integratedTerminal"
    },
    {
      "name": "Sphinx Make Dev (de)",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": ".venv/Scripts/sphinx-build.exe",
      "args": ["-b", "html", "source", "dev_docs/de", "-a", "-E", "-D", "language=de"],
      "console": "integratedTerminal"
    },
    {
      "name": "Sphinx Make Dev (en)",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": ".venv/Scripts/sphinx-build.exe",
      "args": ["-b", "html", "source", "dev_docs/en", "-a", "-E", "-D", "language=en"],
      "console": "integratedTerminal"
    },
    {
      "name": "Sphinx Make Page",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": ".venv/Scripts/sphinx-build.exe",
      "args": ["-b", "html", "source", "docs", "-a", "-E"],
      "console": "integratedTerminal"
    },
    {
      "name": "Sphinx Make Readme",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "program": ".venv/Scripts/sphinx-build.exe",
      "args": ["-b", "rst", "source", "readme", "-a", "-E"],
      "console": "integratedTerminal"
    }
  ],
  "compounds": [
    {
        "name": "Sphinx Make Dev (all)",
        "configurations": ["Sphinx Make Dev (de)", "Sphinx Make Dev (en)"]
    }
]
}
